{
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.ts"
        }
    ],
    "outputDirectory": "dist"
}